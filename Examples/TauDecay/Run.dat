(run){
  % general settings
  EVENTS 100k;
  EVENT_TYPE HadronDecay;

  % specify hadron to be decayed
  DECAYER 15;

  % initialise rest for Sherpa not to complain
  % model parameters
  ME_SIGNAL_GENERATORS Internal;
  SCALES VAR{sqr(91.2)};
  % collider setup
  BEAM_1  11; BEAM_ENERGY_1 7.;
  BEAM_2 -11; BEAM_ENERGY_2 4.;
  #ANALYSIS=Rivet
}(run)

(processes){
  Process 11 -11 -> 13 -13;
  End process;
}(processes)

(analysis){
  BEGIN_RIVET {
  IGNOREBEAMS=1
  -a BABAR_2007_S7266081
  } END_RIVET
}(analysis)
