(run){
  #
  # general settings
  EVENTS 50k; FRAGMENTATION Off;
  EVENT_SEED_MODE 3;
  #
  % Has to stay until soft photons are fixed
  YFS_MODE = 0
  #
  # collider setup
  MODEL HEFT; YUKAWA[11] 1.777;
  BEAM_1 11 45.6; BEAM_2 -11 45.6;
  PDF_SET None;
  #
  # shower setup
  #
  #
  SHOWER_GENERATOR CFP
  #
  SCALES VAR{Abs2(p[2]+p[3])};
  CSS_FS_AS_FAC 1; CSS_FS_PT2MIN 1;

  CSS_MAXEM = 2

  RESPECT_MASSIVE_FLAG 1;
  CSS_TC_ENHANCE 5;           # this is for the 1->3 splittings
  PDF_LIBRARY LHAPDFSherpa; USE_PDF_ALPHAS 7;
  #
  # analysis
  ANALYSIS Rivet
  ANALYSIS_OUTPUT CFPG2_ggg_fromgqq
}(run)

(processes){
  Process 11 -11 -> 21 21
  Order (*,0,2);
  End process;
}(processes)

(analysis){
  BEGIN_RIVET {
  -a JADE_OPAL_2000_S4300807
  } END_RIVET
}(analysis)