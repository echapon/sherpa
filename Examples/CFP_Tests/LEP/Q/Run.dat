(run){
  # general settings
  EVENTS 10M; FRAGMENTATION Off;
  EVENT_SEED_MODE 3;
  RESPECT_MASSIVE_FLAG 1;
  CSS_TC_ENHANCE 5;
  # collider setup
  BEAM_1 11 45.6; BEAM_2 -11 45.6;
  PDF_SET None;
  # shower setup
  SHOWER_GENERATOR Dire;
  SCALES VAR{Abs2(p[2]+p[3])};
  CSS_FS_AS_FAC 1; CSS_FS_PT2MIN 1;
  PDF_LIBRARY LHAPDFSherpa; USE_PDF_ALPHAS 7;
}(run);

(processes){
  Process 11 -11 -> 93 93;
  Order (*,2);
  End process;
}(processes);

(analysis){
  BEGIN_RIVET {
    -a LL_JetRates;
  } END_RIVET;
}(analysis);
