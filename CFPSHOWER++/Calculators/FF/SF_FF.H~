#ifndef CFPSHOWER_Calculators_SF_FF_H
#define CFPSHOWER_Calculators_SF_FF_H

#include "CFPSHOWER++/Calculators/SF_Base.H"

namespace CFPSHOWER {
  class SF_FF : public SF_Base {
  protected:
    virtual double Jacobean(const Splitting & split) const;
  public:
    SF_FF(const Kernel_Info & info);
    virtual void GeneratePoint(Splitting & split) const {
      split.SetZ();
      split.Setphi();
    }
    virtual int Construct(Splitting & split) const;
  };
}

#endif
